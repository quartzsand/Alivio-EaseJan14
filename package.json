{
  "name": "my-app",
  "main": "client/index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --tunnel --clear",
    "dev:mobile": "npx expo start --tunnel --clear",
    "expo:local": "npx expo start --localhost --clear",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "fullstack:dev": "concurrently \"npm run server:dev\" \"npm run expo:local\"",
    "db:push": "drizzle-kit push",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\""
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "^0.4.2",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "2.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.1.7",
    "drizzle-orm": "^0.38.3",
    "expo": "~52.0.31",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.5",
    "expo-font": "~14.0.4",
    "expo-haptics": "~14.0.0",
    "expo-keep-awake": "~14.0.3",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.4",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.7",
    "expo-web-browser": "~14.0.2",
    "express": "^4.18.2",
    "postgres": "^3.4.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.6",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/express": "^4.17.21",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.30.2",
    "esbuild": "^0.24.2",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "~5.3.3"
  },
  "private": true
}
